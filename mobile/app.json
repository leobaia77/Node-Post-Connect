{
  "expo": {
    "name": "GrowthTrack",
    "slug": "growthtrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8F5F0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.growthtrack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E8F5F0"
      },
      "package": "com.growthtrack.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "growthtrack",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
